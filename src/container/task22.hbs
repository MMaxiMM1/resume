<main>
    {{#with page_header}}
    <h1 class="text-center">{{title}}</h1>
    <p class="text-center">{{description}}</p>
    {{/with}}



    <div class="card">
        <div class="card-body">
            {{#with section}}

            {{#with about_us}}

            {{#with title_list}}
            <div class="text-end display-6">{{main}}</div>
            <p class="text-end">{{sub}}</p>
            {{/with}}

            {{#with button}}
            <button class="btn btn-outline-info">{{text}}</button>
            {{/with}}

            {{/with}}

        </div>
    </div>

    <br>


    <div class="card">
        <div class="card-body">
            {{#with advantage}}
            <div class="display-5 text-muted">{{title}}</div>
        </div>
        <div class="card">
            <div class="card-body">
                {{#with efficiency}}
                <div class="display-4 text-muted">{{title}}</div>
                <br>
                <h6>{{sub_title}}</h6>
                <br>
                <p>{{description}}</p>
                <div class="btn btn-outline-warning">{{button}}</div>

                {{/with}}
            </div>
        </div>



        <br>

        <div class="card">
            <div class="card=body">
                {{#with innovation}}
                <div class="display-4 text-muted text-center">{{title}}</div>
                <br>
                <h6 class="text-center">{{sub_title}}</h6>
                <p class="text-center">{{description}}</p>
                <div class="btn btn-outline-warning">{{button}}</div>


                {{/with}}



            </div>
        </div>

        <br>

        <div class="card">
            <div class="card-body">
                {{#with innovation}}
                <div class="display-4 text-muted text-end">{{title}}</div>
                <br>
                <h6 class="text-end">{{sub_title}}</h6>
                <br>
                <p class="text-end">{{description}}</p>
                <div class="btn btn-outline-warning">{{button}}</div>
                {{/with}}

            </div>
        </div>
        {{/with}}
</main>

<br>

<div class="card">
    <div class="card-body">
        {{#with agreement}}
        <div class="display-5">{{title}}</div>

        <div class="card">
            <div class="card-body">
                {{#with info}}
                <h5>{{sub_title}}</h5>

                <div class="card">
                    <div class="card-body">
                        {{#with detail}}
                        <p>{{text}}</p>
                        <div class="btn btn-success">{{button}}</div>
                        {{/with}}
                    </div>
                </div>
                {{/with}}
            </div>
        </div>
        {{/with}}
    </div>
</div>
{{/with}}